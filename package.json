{
  "name": "styled-inline-components",
  "version": "0.0.1",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "build": "del lib && rollup -c",
    "jest": "jest",
    "lint": "eslint src",
    "pub": "NODE_ENV=production npm run build && npm publish",
