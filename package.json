  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romellogood/styled-inline-components.git"
  },
  "bugs": {
    "url": "https://github.com/romellogood/styled-inline-components/issues"
  },
  "keywords": [
    "cssinjs",
